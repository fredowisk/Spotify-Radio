{
  "name": "spotify-radio",
  "version": "1.0.0",
  "description": "Seja bem vindo(a) à sexta Semana Javascript Expert. Este é o código inicial para iniciar nossa jornada.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "set NODE_OPTIONS='--experimental-vm-modules --no-warning' && jest --no-cache",
    "test-watch": "set LOG_DISABLED=true && set NODE_OPTIONS='--experimental-vm-modules --no-warning' && jest --no-cache --watchAll",
    "test-coverage": "set NODE_OPTIONS='--experimental-vm-modules --no-warning' && jest --no-cache --coverage tests/unit",
    "dev-docker": "docker-compose up dev"
  },
  "engines": {
    "node": "16.9.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3"
  }
}
